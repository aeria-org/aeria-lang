collection User {
  icon "users"

  filtersPresets {
    active {
      name "Ativas"
      filters @mongo {
        active: true,
      }
      @end
      badgeFunction "count"
    }
  }

  properties {
    username      str
    active        bool
  }

  search {
    indexes {
      username
    }
    placeholder "Search..."
  }

  layout {
    active {
      component {
        name "Label"
      }
    }
  }
}
