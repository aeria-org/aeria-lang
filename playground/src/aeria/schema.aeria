collection Pet {
  properties {
    name str
    age int
    breed enum @options(["dog", "cat", "bird"])
  }
}

collection Person {
  properties {
    name str
    age int
    age int @minimum(10)
    name str @minLength(10) @maxLength(30)
    options {
      disabled bool
      pets []Pet
    }
  }
}
